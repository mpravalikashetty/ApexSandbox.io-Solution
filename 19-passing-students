public Boolean isPassed(Boolean passedExam, Boolean passedAssignments, Boolean passedProject) {
    //code here
   /*if((passedExam && passedAssignments) || (passedExam && passedProject) ||
   (passedAssignments && passedProject)){
       return true;
   }else{
       return false;
   }*/
   Integer result=0;
   if(passedExam){
       result=result+1;
   }
   if(passedAssignments){
       result=result+1;
   }
   if(passedProject){
       result=result+1;
   }
   if(result>=2){
       return true;
   }else{
       return false;
   }
}